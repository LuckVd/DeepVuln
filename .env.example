# DeepVuln Environment Configuration
# Copy this file to .env and customize as needed

# Workspace Configuration
DEEPVULN_WORKSPACE_BASE_DIR=
DEEPVULN_WORKSPACE_MAX_WORKSPACES=10
DEEPVULN_WORKSPACE_AUTO_CLEANUP=true
DEEPVULN_WORKSPACE_PREFIX=deepvuln_

# Git Configuration
DEEPVULN_GIT_CLONE_TIMEOUT=300
DEEPVULN_GIT_DEFAULT_DEPTH=0
DEEPVULN_GIT_RETRY_ATTEMPTS=3
DEEPVULN_GIT_RETRY_DELAY=5
DEEPVULN_GIT_VERIFY_SSL=true

# Logging Configuration
DEEPVULN_LOGGING_LEVEL=INFO
DEEPVULN_LOGGING_FILE=
DEEPVULN_LOGGING_USE_RICH=true
